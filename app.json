{
  "expo": {
    "name": "galio-native-kit",
    "description": "Build amazing apps in one codebase. //wip from sdk30 to sdk33",
    "slug": "galio-native-kit",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./public/images/flourish.png",
      "resizeMode": "stretch",
      "backgroundColor": "#77C054"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/galio-org/galio"
  }
}
